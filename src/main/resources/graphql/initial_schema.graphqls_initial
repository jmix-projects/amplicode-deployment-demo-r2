## Owner
#input OwnerInput {
#    id: ID
#    firstName: String
#    lastName: String
#    address: String
#    city: String
#    email: String
#    telephone: String
#}
#
#type Owner {
#    id: ID
#    firstName: String!
#    lastName: String!
#    address: String!
#    city: String!
#    email: String
#    telephone: String
#}
